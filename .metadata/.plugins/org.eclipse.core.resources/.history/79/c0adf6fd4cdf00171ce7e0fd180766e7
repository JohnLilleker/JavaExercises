package paintExercise;

public class Main {

	public static void main(String[] args) {
		PaintWizard paintWizard = new PaintWizard();

		for (double i = 0; i < 10000; i += 150) {
			System.out.println("Size of floor = " + i + " m2");
			System.out.println("Best paint is " + paintWizard.bestForFloor(i));
			System.out.println();
		}

		// System.out.println(paintWizard.bestForFloor(8));
	}

}
