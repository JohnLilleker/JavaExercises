
public class Intermediate {
	public static void main(String [] args) {
		Intermediate i = new Intermediate();
		
	}
	
	public int blackjack(int x, int y) {
		int diff1 = 21 - x;
		int diff2 = 21 - y;
		if (diff1 < 0 && diff2 < 0) return 0;
		
		if (diff1 < 0) return y;
		if (diff2 < 0) return x;
		
		return (diff1 < diff2) ? x : y;
		
		
	}
}
