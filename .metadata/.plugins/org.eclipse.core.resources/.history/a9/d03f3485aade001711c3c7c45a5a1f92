
public class Tank extends Vehicle {

	private int calibre;

	public Tank(int calibre, int length, int weight, String id) {
		super(id, 2, length, weight);
		this.calibre = calibre;
	}

	public int getCalibre() {
		return calibre;
	}

	@Override
	public String toString() {
		return "Tank [calibre=" + calibre + ", wheels=" + wheels + ", length=" + length + ", weight=" + weight + ", id="
				+ id + "]";
	}

}
