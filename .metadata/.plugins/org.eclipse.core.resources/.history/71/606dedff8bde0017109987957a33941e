
public abstract class Vehicle {
	protected int wheels;
	protected int length;
	protected int weight;

	public Vehicle(int wheels, int length, int weight) {
		this.wheels = wheels;
		this.length = length;
		this.weight = weight;
	}
	// do some stuff...
	
	public int getWheels() {
		return wheels;
	}
	
	public int getLength() {
		return length;
	}
	
	public int getWeight() {
		return weight;
	}
	
}
